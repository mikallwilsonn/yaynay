{
  "name": "emaily",
  "version": "1.0.0",
  "description": "FullStack Node, Express, React/Redux and MongoDB application to send emails for user feedback",
  "main": "index.js",
  "engines": {
    "node": "9.11.1",
    "npm": "5.6.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikallwilsonn/emaily.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikallwilsonn/emaily/issues"
  },
  "homepage": "https://github.com/mikallwilsonn/emaily#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "mongoose": "^5.4.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "stripe": "^6.20.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
